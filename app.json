
{
  "expo": {
    "name": "Bee Good",
    "slug": "bee-good",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bee-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/bee-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F6B93B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.beegood.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/bee-adaptive-icon.png",
        "backgroundColor": "#F6B93B"
      },
      "package": "com.beegood.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
